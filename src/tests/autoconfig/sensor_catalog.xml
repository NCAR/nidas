<?xml version="1.0" encoding="UTF-8"?>
<sensorcatalog>
    <!-- Legacy DSMSerialSensor Dynld implementation -->
    <serialSensor 
        ID="LegacySerialSensor" 
        class="DSMSerialSensor" 
        init_string="DSMSensorTestInitString"
		baud="9600" 
        parity="even" 
        databits="7" 
        stopbits="1">
        <!-- 838.26\r\n -->
        <sample 
            id="1" 
            scanfFormat="%f" 
            rate="1">
            <variable 
                name="P" 
                units="mb"
                longname="Barometric Pressure, Vaisala PTB 210"
                plotrange="$P_RANGE"/>
        </sample>
        <message 
            separator="\n" 
            position="end" 
            length="0"/>
    </serialSensor>

