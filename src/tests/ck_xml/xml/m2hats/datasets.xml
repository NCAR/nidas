        <envvar name="WIND3D_HORIZ_COORDINATES" value="geographic"/>
        <envvar name="WIND3D_HORIZ_ROTATION" value="true"/>
        <envvar name="WIND3D_TILT_CORRECTION" value="false"/>
        <envvar name="CSAT3_CKCNTR" value="true"/>
