	    <output class="RawSampleOutputStream">
                <socket type="server" port="30000"/>
            </output>
	    <output class="RawSampleOutputStream">
		<fileset dir="$DATAMNT/projects/${PROJECT}/raw_data"
		    file="${DSM}_%Y%m%d_%H%M%S.dat"
                    length="43200">
		    <mount dir="$DATAMNT" dev="/dev/sda1" type="ext3"/>
		</fileset>
