#
# Simple installer.
#
# The mesa_fpga_file.bit file is generated By Mike Spowart.  It is
# generated from the anythingIO_260x.vhd file by a windows application.
#
Import('env')

firmwares = Split("""
    mesa_fpga_file.bit
""")

inst = env.Install('$PREFIX/firmware', firmwares)
env.Alias('install', ['$PREFIX/firmware'])

env.Clean(inst, [
    '$PREFIX/firmware/' + l for l in firmwares
    ])
