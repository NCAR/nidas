    </arincSensor>

    <serialSensor ID="Garmin_GPS" class="GPS_NMEA_Serial"
        baud="4800" parity="none" databits="8" stopbits="1">

        <!-- GGA record -->
        <sample id="1" rate="1">
	    <variable name="GGSECSDAY" units="sec" longname="seconds of day"/>
	    <variable name="GGLAT" units="degree_N" longname="GPS Latitude"/>
	    <variable name="GGLON" units="degree_E" longname="GPS Longitude"/>
	    <variable name="GGQUAL" units="none" longname="GPS Qual, 0=inval,1=GPS,2=DGPS"/>
	    <variable name="GGNSAT" units="count" longname="Number of GPS satellites tracked"/>
	    <variable name="GGHORDIL" units="none" longname="Horizontal dilution of position"/>
	    <variable name="GGALT" units="m" longname="GPS Altitude (MSL)"/>
	    <variable name="GGEOIDHT" units="m" longname="Height of geoid (MSL) above WGS84 ellipsoid"/>
	    <variable name="GGDAGE" units="s" longname="time in seconds since last DGPS update"/>
	    <variable name="GGREFID" units="none" longname="DGPS station ID number"/>
        </sample>
        <!-- RMC record -->
        <sample id="2" rate="1">
	    <variable name="GGSTATUS" units="none" longname="GPS rcvr status: 1=OK(A), 0=warning(V)"/>
	    <variable name="GGSPD" units="m/s" longname="Speed over ground"/>
	    <variable name="GGTRK" units="degree_T" longname="Course made good"/>
	    <variable name="GGVEW" units="m/s" longname="East/West velocity"/>
	    <variable name="GGVNS" units="m/s" longname="North/South velocity"/>
	    <variable name="GGDAY" units="day" longname="Day of month (1-31)"/>
	    <variable name="GGMONTH" units="month" longname="Month of year (1-12)"/>
	    <variable name="GGYEAR" units="year" longname="2 digit year"/>
        </sample>
        <message separator="\n" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="DewPointer" class="DSMSerialSensor"
        baud="9600" parity="none" databits="8" stopbits="1">
        <sample id="1" scanfFormat="%*d,%f" rate="1"/>
        <message separator="\n" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="Mensor_6100" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
	<!-- Turn off internal filtering on the Mensor:
		#1FL ?	query the value (factory default is 90%)
		#1FL 0	set to 0
		#1FL ?
		#1SAVE 	save to EEPROM
	-->
        <sample id="1" scanfFormat="%*1d%f"/>
        <message separator="\n" position="end" length="0"/>
        <prompt string="#1?\r" rate="50"/>
    </serialSensor>

    <serialSensor ID="Paro_DigiQuartz_1000" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
	<!-- initialization commands for the Paro:
	  unprompted mode, set rate with PI (and in sample tag)
	  	If we set MD=2, don't need P4 init string
	  *9900BR=57600
	  *0100EW*0100MD=2	turn off  Model 715 output, unprompted
	  *0100EW*0100UN=2	units=millibars
	  *0100EW*0100PI=10	rate=50
	-->

        <sample id="1" scanfFormat="*%*2d%*2d%f" rate="50">
	</sample>
        <message separator="\n" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="Water_CN_Counter" class="DSMSerialSensor"
        baud="115200" parity="none" databits="8" stopbits="1"
        init_string="SM,4,1\r">
        <sample id="1" rate="10" scanfFormat="D,%f,%f,%f,%f,%f,%f,%f,%f">
	    <variable name="MCN_WCN" units="none" longname="WCN Current Operating Mode"/>
	    <variable name="SFLCN_WCN" units="none" longname="WCN Status Flags"/>
	    <variable name="CONCN_WCN" units="#/cm3" longname="WCN Aggregated Concentration"/>
	    <variable name="ETCN_WCN" units="second" longname="WCN Sample Time (Elapsed Time)"/>
	    <variable name="LTCN_WCN" units="second" longname="WCN Live Time"/>
	    <variable name="CNTS_WCN" units="count" longname="WCN Accumulated Particle Counts"/>
	    <variable name="PCN_WCN" units="mbar" longname="WCN Absolute Pressure"/>
	    <variable name="RPCN_WCN" units="none" longname="WCN Raw Photometric Value"/>
        </sample>
        <sample id="2" rate="10" scanfFormat="S,%f,%f,%f,%f,%f,%f,%f">
	    <variable name="FCN_WCN" units="cm3/min" longname="WCN Sample Flow Rate"/>
	    <variable name="TFCN_WCN" units="cm3/min" longname="WCN Total Flow Rate"/>
	    <variable name="SFCN_WCN" units="cm3/min" longname="WCN Sheath Flow Rate"/>
	    <variable name="APCN_WCN" units="mbar" longname="WCN Absolute Pressure"/>
	    <variable name="STCN_WCN" units="deg_C" longname="WCN Saturator Temperature"/>
	    <variable name="GTCN_WCN" units="deg_C" longname="WCN Growth Tube Temperature"/>
	    <variable name="OTCN_WCN" units="deg_C" longname="WCN Optics Temperature"/>
        </sample>
        <message separator="\r" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="TDLH2O" class="DSMSerialSensor"
        baud="115200" parity="none" databits="8" stopbits="1">
        <sample id="1" rate="20" scanfFormat="%*f %*f %f %f %*f %*f %f %f %f %f %f %f %f %f">
	    <variable name="PCELL_LHS" units="mbar" longname="Sample Cell Pressure"/>
	    <variable name="TCELL_LHS" units="deg_C" longname="Sample Cell Temperature"/>
	    <variable name="PP2F_LHS" units="A/D cnts" longname="2f Signal Amplitude"/>
	    <variable name="PWR_LHS" units="A/D cnts" longname="Laser Power"/>
	    <variable name="IDX_LHS" units="A/D cnts" longname="Spectral Line Position"/>
	    <variable name="DK_LHS" units="A/D cnts" longname="Zero Level"/>
	    <variable name="PP2F_LHL" units="A/D cnts" longname="2f Signal Amplitude"/>
	    <variable name="PWR_LHL" units="A/D cnts" longname="Laser Power"/>
	    <variable name="IDX_LHL" units="A/D cnts" longname="Spectral Line Position"/>
	    <variable name="DK_LHL" units="A/D cnts" longname="Zero Level"/>
        </sample>
        <message separator="\n" position="end" length="0"/>
    </serialSensor>

</sensorcatalog>
