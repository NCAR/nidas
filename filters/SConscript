#
# Simple installer.
#
# The mesa_fpga_file.bit file is generated By Mike Spowart.  It is
# generated from the anythingIO_260x.vhd file by a windows application.
#
Import('env')

a2dfilts = Split("""
    fir10KHz.cfg
    fir1KHz.cfg
    fir500Hz.cfg
    fir5KHz.cfg
""")

env.Install('$PREFIX/firmware', a2dfilts)

env.Clean(['install','arm_install'], [
    '$PREFIX/firmware/' + l for l in a2dfilts
    ])


                                                
